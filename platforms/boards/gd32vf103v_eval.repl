flash0: Memory.MappedMemory @ sysbus 0x8000000
    size: 0x20000

sram0: Memory.MappedMemory @ sysbus 0x20000000
    size: 0x8000

// autogenerated

rcu: Python.PythonPeripheral @ sysbus 0x40021000
    size: 0x400
    initable: false
    script: "request.value = 0xFFFFFFFF"

usart0: UART.GD32_UART @ sysbus 0x40013800

usart1: UART.GD32_UART @ sysbus 0x40004400

cpu: CPU.RiscV32 @ sysbus
    cpuType: "rv32imac_zicsr_zifencei"
    timeProvider: empty
    hartId: 0
    privilegeArchitecture: PrivilegeArchitecture.Priv1_11
    init:
        RegisterCustomCSR "MSUBM" 0x7C4 Machine
        RegisterCustomCSR "MMISC_CTL" 0x7D0 Machine
        RegisterCustomCSR "MTVT2" 0x7EC Machine

clic: IRQControllers.CoreLocalInterruptController @ {
    sysbus <0xd2000000, +0x10000>;
    cpu <0x1000, +0x4A1>
}
    cpu: cpu
